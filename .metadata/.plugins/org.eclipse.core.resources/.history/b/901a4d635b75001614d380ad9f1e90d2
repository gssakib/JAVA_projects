package demo;
import java.util.*;

public class Sum {

	public static void main(String[] args) {
		Scanner in =  new Scanner(System.in);
		int num = in.nextInt();
		int num_1 = num;
		int third = num_1 % 10;
		num_1 = num_1 / 10;
		int second = num_1 % 10;
		num_1 = num_1 / 10;
		
		System.out.println("The sum of the digits in the number:"+ num + " is: "+ (third + second + num_1) );
		
		// TODO Auto-generated method stub

	}

}
