package demo;

public class Kg2lb2kg {

	public static void main(String[] args) {
		
		int j = 0;
		System.out.println("kilograms      Pounds        Pounds       kilograms");
		
		for(int i = 1; i <= 199; i++){
			
			if((i % 2) != 0){
				if(i < 11){
					j = (20 + 5*(i-1));
					System.out.println("    " + i+"           " + (float)(i*2.2)+"  " + (float)j + "   " + (float)(1/2.2)*j );
				}else{
					j = (20 + 5*(i-1));
					System.out.println("    " + i+"          " + (float)(i*2.2)+ "  "+ (float)j+"    " + (float)(1/2.2)*j);
				}
				
				
			}
			
			
		}
		
		
		}

	}
