package demo;
import java.util.Scanner;

public class Test2{
	public static void main(String[] args){
		Scanner stdin = new Scanner(System.in);
		int num = stdin.nextInt();
		int temp = 0;
		int count = 0;
		while(num > 0){
			if(temp == num){
				count++;
				temp = num; 
				System.out.println(temp);
			}

			num = stdin.nextInt();
		}

		System.out.println(count);

		}

}
