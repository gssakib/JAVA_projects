package demo;

public class pyramid {

	public static void main(String[] args) {
		
		Pyramid(9, '#');
		
	}
	
	public static void Pyramid(int base, char sym){
		
		for(int j = 0; j < base; j++){
			for(int i = 0; i < (base - j); i++){
				for(int k = 0; k< )
				System.out.print(sym);
			}
			System.out.println();
		}
		
		
		
		
	}

}
