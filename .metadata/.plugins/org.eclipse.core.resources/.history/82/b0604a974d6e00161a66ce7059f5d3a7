package demo;
import java.util.Scanner;

public class ComputeArea {

	public static void main(String[] args) {
		double radius;
		double area;
		radius = 20;
		area = radius * radius * Math.PI  
		System.out.println("The area of the circle of radius" + radius + "is : " + area);
		
		
